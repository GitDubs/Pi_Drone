<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    tools:context="com.example.keablerman.myapplication.FlightDeck"
    tools:layout_editor_absoluteY="73dp"
    tools:layout_editor_absoluteX="0dp">


    <ImageButton
        android:id="@+id/down_button"
        android:layout_width="0dp"
        android:layout_height="71dp"
        android:layout_marginBottom="77dp"
        android:rotation="180"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="@+id/up_button"
        app:layout_constraintRight_toRightOf="@+id/up_button"
        app:srcCompat="@drawable/arrow_up"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintHorizontal_bias="1.0" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="60dp"
        android:layout_height="66dp"
        android:rotation="90"
        app:srcCompat="@drawable/arrow_up"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="63dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView6"
        android:layout_marginTop="63dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="65dp"
        app:layout_constraintTop_toTopOf="@+id/imageView5"
        app:layout_constraintLeft_toRightOf="@+id/imageView4"
        android:layout_marginLeft="63dp"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="0dp"
        android:layout_height="66dp"
        android:rotation="180"
        app:srcCompat="@drawable/arrow_up"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="67dp"
        tools:layout_constraintRight_creator="1"
        android:layout_marginStart="60dp"
        android:layout_marginEnd="59dp"
        app:layout_constraintRight_toRightOf="@+id/imageView7"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/imageView4"
        android:layout_marginLeft="60dp"
        android:layout_marginRight="59dp"
        app:layout_constraintHorizontal_bias="0.0" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="60dp"
        android:layout_height="0dp"
        android:rotation="270"
        app:srcCompat="@drawable/arrow_up"
        android:layout_marginStart="43dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/blueCircle"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/blueCircle"
        android:layout_marginLeft="43dp"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="63dp"
        android:layout_height="64dp"
        app:srcCompat="@drawable/arrow_up"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/imageView6"
        app:layout_constraintRight_toRightOf="@+id/blueCircle"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="64dp"
        app:layout_constraintLeft_toLeftOf="@+id/blueCircle"
        app:layout_constraintHorizontal_bias="0.0" />

    <ImageView
        android:id="@+id/blueCircle"
        android:layout_width="63dp"
        android:layout_height="0dp"
        android:layout_marginEnd="60dp"
        android:layout_marginLeft="60dp"
        android:layout_marginRight="60dp"
        android:layout_marginStart="60dp"
        android:adjustViewBounds="false"
        app:layout_constraintBottom_toBottomOf="@+id/imageView7"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="@+id/imageView4"
        app:layout_constraintRight_toRightOf="@+id/imageView7"
        app:layout_constraintTop_toBottomOf="@+id/imageView5"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/blue_circle"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <ImageButton
        android:id="@+id/up_button"
        android:layout_width="66dp"
        android:layout_height="71dp"
        app:srcCompat="@drawable/arrow_up"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/down_button"
        android:layout_marginEnd="145dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="44dp"
        android:layout_marginRight="296dp" />

    <Button
        android:id="@+id/kill"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="-96dp"
        android:layout_marginStart="58dp"
        android:background="@android:color/holo_red_dark"
        android:onClick="onClick"
        android:text="Kill Motors"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/imageView7"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintTop_creator="1" />

    <SeekBar
        android:id="@+id/speedDelta"
        android:layout_width="152dp"
        android:layout_height="23dp"
        android:max="9"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.873"
        app:layout_constraintLeft_toLeftOf="@+id/kill"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.833"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"
        app:layout_constraintRight_toLeftOf="@+id/down_button"
        android:layout_marginRight="8dp" />


</android.support.constraint.ConstraintLayout>
