<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    tools:context="com.example.keablerman.myapplication.FlightDeck"
    tools:layout_editor_absoluteY="73dp"
    tools:layout_editor_absoluteX="0dp">




    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="85dp"
        android:layout_height="85dp"
        app:layout_constraintBottom_toTopOf="@+id/pitch_roll"
        app:layout_constraintStart_toStartOf="@+id/pitch_roll"
        app:srcCompat="@drawable/arrow_up"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="85dp"
        android:layout_height="85dp"
        app:layout_constraintBottom_toTopOf="@+id/throttle_yaw"
        app:layout_constraintStart_toStartOf="@+id/throttle_yaw"
        app:srcCompat="@drawable/arrow_up"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:rotation="90"
        app:layout_constraintStart_toEndOf="@+id/pitch_roll"
        app:layout_constraintTop_toTopOf="@+id/pitch_roll"
        app:srcCompat="@drawable/arrow_up"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintTop_creator="1" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:rotation="90"
        app:layout_constraintStart_toEndOf="@+id/throttle_yaw"
        app:layout_constraintTop_toTopOf="@+id/throttle_yaw"
        app:srcCompat="@drawable/arrow_up"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintTop_creator="1" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:rotation="270"
        app:layout_constraintEnd_toStartOf="@+id/pitch_roll"
        app:layout_constraintTop_toTopOf="@+id/pitch_roll"
        app:srcCompat="@drawable/arrow_up"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintTop_creator="1" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:rotation="270"
        app:layout_constraintEnd_toStartOf="@+id/throttle_yaw"
        app:layout_constraintTop_toTopOf="@+id/throttle_yaw"
        app:srcCompat="@drawable/arrow_up"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintTop_creator="1" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:rotation="180"
        app:layout_constraintStart_toStartOf="@+id/pitch_roll"
        app:layout_constraintTop_toBottomOf="@+id/pitch_roll"
        app:srcCompat="@drawable/arrow_up"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:rotation="180"
        app:layout_constraintStart_toStartOf="@+id/throttle_yaw"
        app:layout_constraintTop_toBottomOf="@+id/throttle_yaw"
        app:srcCompat="@drawable/arrow_up"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1" />

    <ImageView
        android:id="@+id/pitch_roll"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:layout_marginEnd="180dp"
        android:layout_marginRight="180dp"
        android:adjustViewBounds="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.991"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.533"
        app:srcCompat="@drawable/blue_circle"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <ImageView
        android:id="@+id/throttle_yaw"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:layout_marginEnd="180dp"
        android:layout_marginRight="180dp"
        android:adjustViewBounds="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.328"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.533"
        app:srcCompat="@drawable/blue_circle"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <Button
        android:id="@+id/kill"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="108dp"
        android:background="@android:color/holo_red_dark"
        android:onClick="onClick"
        android:text="Kill Motors"
        app:layout_constraintBottom_toTopOf="@+id/pitch_roll"
        app:layout_constraintStart_toEndOf="@+id/pitch_roll"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintTop_creator="1" />


</android.support.constraint.ConstraintLayout>
